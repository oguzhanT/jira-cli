{
    "name": "oguzhantogay/jira-cli",
    "description": "A PHP CLI tool to interact with Jira issues directly from the terminal.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "OguzhanTogay\\JiraCLI\\": "src/"
        }
    },
    "bin": [
        "bin/jira-cli"
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.9",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.64"
    },
    "scripts": {
        "test": "phpunit",
        "cs-fix": "php-cs-fixer fix"
    },
    "authors": [
